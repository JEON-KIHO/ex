<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper_oracle.CalendarMapper">
<insert id="insert">
	insert into day(day) values(#{date})
</insert>

<select id="list" resultType="com.example.domain.DayVO">
	select day from day where day like #{keyword} || '%' order by day
</select>

<select id="total" resultType="int">
	select count(*) from day where day like #{keyword} || '%'
</select>
</mapper>