package com.example.domain;

import java.util.Date;

public class DayVO {
	private Date day;
	private int wom;

	public int getDow() {
		return wom;
	}

	public void setDow(int dow) {
		this.wom = dow;
	}

	public Date getDay() {
		return day;
	}

	public void setDay(Date day) {
		this.day = day;
	}
}
